{
    "version": 2,
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        },
        {
            "src": "LOGTRACK/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles_build/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "LOGTRACK/wsgi.py"
        }
    ]
}